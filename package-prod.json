{
  "name": "me-view",
  "version": "1.2.6",
  "private": false,
  "main": "lib/me-view.common.js",
  "files": [
    "lib",
    "src",
    "theme",
    "packages"
  ],
  "scripts": {
    "serve": "vue-cli-service serve --fix",
    "build:tools": "webpack --config build/webpack.Tools.js",
    "build:common": "webpack --config build/webpack.common.js",
    "build:theme": "gulp build --gulpfile src/theme/gulpfile.js && cp-cli src/theme/lib theme",
    "build:component": "webpack --config build/webpack.component.js",
    "package": "vue-cli-service build --dest lib --target lib ./src/index.js",
    "build:all": "npm run package && npm run build:component && npm run build:tools && npm run build:theme && npm run build:common",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "async-validator": "^3.2.4",
    "axios": "^0.18.1",
    "clipboard": "^2.0.6",
    "core-js": "^3.6.5",
    "highlight.js": "^9.18.1",
    "qs": "^6.9.3",
    "vue": "^2.6.11",
    "vue-router": "^3.1.6"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.3.1",
    "@vue/cli-plugin-eslint": "^4.3.1",
    "@vue/cli-plugin-router": "^4.3.1",
    "@vue/cli-service": "^4.3.1",
    "@vue/eslint-config-standard": "^5.1.0",
    "babel-eslint": "^10.1.0",
    "babel-polyfill": "^6.26.0",
    "eslint": "^6.7.2",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.0",
    "eslint-plugin-vue": "^6.2.2",
    "lint-staged": "^9.5.0",
    "markdown-it": "^8.4.2",
    "markdown-it-anchor": "^5.2.7",
    "markdown-it-container": "^2.0.0",
    "sass": "^1.26.3",
    "sass-loader": "^8.0.2",
    "vue-markdown-loader": "^2.4.1",
    "vue-template-compiler": "^2.6.11",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,vue}": [
      "vue-cli-service lint",
      "git add"
    ]
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
