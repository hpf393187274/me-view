{
  "name": "view-vue",
  "version": "1.0.4",
  "private": false,
  "main": "dist/view-vue.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-bundle": "vue-cli-service build --target lib --name view-vue ./src/components/index.js",
    "npm-publish": "npm publish",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "autoprefixer": "^9.3.1",
    "vue": "^2.5.17",
    "webpack": "^4.23.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.0.5",
    "@vue/cli-plugin-eslint": "^3.0.5",
    "@vue/cli-service": "^3.0.5",
    "vue-template-compiler": "^2.5.17"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/strongly-recommended",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "vue/html-closing-bracket-newline": [
        "error",
        {
          "multiline": "never"
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "description": "## Project setup ``` npm install ```",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hpf393187274/view-vue.git"
  },
  "author": "hpf393187274",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hpf393187274/view-vue/issues"
  },
  "homepage": "https://github.com/hpf393187274/view-vue#readme"
}
