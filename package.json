{
  "name": "me-view",
  "version": "1.0.12",
  "private": false,
  "main": "dist/lib/index.common.js",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --dest dist/build",
    "package": "vue-cli-service build --dest dist/lib --target lib --name index ./src/index.js",
    "package-theme": "vue-cli-service build --dest dist/theme --target lib --name index ./src/index.js",
    "package-example": "vue-cli-service build --dest dist/example --target lib --name index ./src/example.js",
    "npm-publish": "npm publish",
    "lint": "vue-cli-service lint",
    "docs:dev": "vuepress dev docs",
    "docs:build": "vuepress build docs"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "core-js": "^2.6.5",
    "echarts": "^4.2.1",
    "vue": "^2.6.10",
    "vue-echarts": "^4.0.1",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.6.0",
    "@vue/cli-plugin-eslint": "^3.6.0",
    "@vue/cli-service": "^3.6.0",
    "autoprefixer": "^9.5.1",
    "babel-eslint": "^10.0.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "me-view": "^1.0.11",
    "postcss": "^7.0.16",
    "postcss-loader": "^3.0.0",
    "vue-template-compiler": "^2.5.21",
    "vuepress": "^0.14.11"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off",
      "no-debugger": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "description": "## Project setup ``` npm install ```",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hpf393187274/me-view.git"
  },
  "author": "hpf393187274",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hpf393187274/me-view/issues"
  },
  "homepage": "https://github.com/hpf393187274/me-view#readme"
}
